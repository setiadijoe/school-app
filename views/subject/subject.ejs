<!DOCTYPE html>
<html lang="en">

<head>
<%- include ../partial/head%>
</head>

<body>
    <h1 align="center">Subject Data</h1>
<%- include ../partial/nav-bar%>
    <table border="1" align="center">
        <thead>
            <th>ID</th>
            <th>SUBJECT NAME</th>
            <th>TEACHERS</th>
            <th>ACTION</th>
        </thead>
        <%data.forEach( r =>{%>
            <tbody>
                <td>
                    <%=r.id%>
                </td>
                <td>
                    <%=r.subject_name%>
                </td>
                <td>
                    <%if (r.dataValues.fullname.length > 0) {%>
                        <%r.dataValues.fullname.forEach((z,i=1)=>{%>
                    <%=i+1+'.'+z%><br>
                            <%})%>
                            <%}%>
                </td>
                <td>
                    <a href="/subject/<%=r.id%>/enrolledstudents">Enrolled Students</a>
                </td>
            </tbody>
            <%})%>
    </table>
</body>

</html>